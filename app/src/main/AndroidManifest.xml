<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.interviewreminderapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InterViewReminderApp">
        <activity
            android:name="activitiy.HomeScreen"
            android:exported="false" />
        <activity
            android:name="activitiy.LoginActivity"
            android:exported="false" />
        <activity
            android:name="activitiy.AddInterview"
            android:exported="false" />
        <activity
            android:name="activitiy.DashBoardActivity"
            android:exported="false" />
        <activity
            android:name="activitiy.SplashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>